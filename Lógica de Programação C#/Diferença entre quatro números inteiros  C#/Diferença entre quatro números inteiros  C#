using System;


namespace Course
{
    class Program
    {
        public static void Main(string[] args)
        {

            Console.WriteLine("Estreva quatro númaros");

            int n1 = int.Parce(Console.ReadLine());
            int n2 = int.Parce(Console.ReadLine());
            int n3 = int.Parce(Console.ReadLine());
            int n4 = int.Parce(Console.ReadLine());

            int diferenca = (n1 * n2) - (n3 * n4);

            Console.WriteLine("Diferença= " + difernca);
        }
    }
}
